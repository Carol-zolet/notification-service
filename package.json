{
    "name":  "microsservicos-alerta",
    "version":  "1.0.0",
    "private":  true,
    "type":  "commonjs",
    "scripts":  {
                    "dev":  "ts-node-dev --respawn --transpile-only src/infra/main.ts",
                    "start":  "node dist/infra/main.js",
                    "build":  "tsc -p tsconfig.json",
                    "prisma:generate":  "npx prisma generate --schema=./src/infra/database/prisma/schema.prisma",
                    "prisma:migrate":  "npx prisma migrate dev --schema=./src/infra/database/prisma/schema.prisma",
                    "prisma:seed":  "ts-node --project tsconfig.json prisma/seed.ts",
                    "test":  "jest --runInBand"
                },
    "dependencies":  {
                         "@prisma/client":  "^5.10.0",
                         "cors":  "^2.8.5",
                         "csv-parser":  "^3.0.0",
                         "dotenv":  "^16.4.5",
                         "express":  "^4.19.2",
                         "multer":  "^1.4.5-lts.1",
                         "node-cron":  "^3.0.3",
                         "nodemailer":  "^6.10.1",
                         "zod":  "^3.22.4"
                     },
    "devDependencies":  {
                            "@types/cors":  "^2.8.19",
                            "@types/express":  "^4.17.21",
                            "@types/jest":  "^29.5.12",
                            "@types/multer":  "^1.4.11",
                            "@types/node":  "^20.19.24",
                            "@types/node-cron":  "^3.0.11",
                            "@types/nodemailer":  "^6.4.14",
                            "@types/supertest":  "^6.0.2",
                            "jest":  "^29.7.0",
                            "prisma":  "^5.10.0",
                            "supertest":  "^6.3.4",
                            "ts-jest":  "^29.1.2",
                            "ts-node":  "^10.9.2",
                            "ts-node-dev":  "^2.0.0",
                            "typescript":  "^5.9.3"
                        },
    "prisma":  {
                   "seed":  "ts-node prisma/seed.ts"
               }
}
